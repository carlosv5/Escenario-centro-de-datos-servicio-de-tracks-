<div class="container mainContainer">
	<div class="jumbotron ">
		<div class="row">
			<h1 class="font1 center col-lg-12 col-md-12 col-sm-12 col-xs-12">Tracks</h1>
			<div class="row col-lg-12 col-md-12 col-sm-12 col-xs-6">
				<%for (var t in tracks) {%>
					<div class=" col-lg-3 col-md-3 col-sm-4 col-xs-12">
						<a class="track" href='/tracks/<%=tracks[t].id%>'>
						<img class="img" src="<%=tracks[t].urlImg%>">
						<p><%=tracks[t].name.split('.')[0]%></p></a>
					</div>
				<%}%>
			</div>
		</div>
	</div>
</div>
